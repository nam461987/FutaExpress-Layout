<section id="personal-regis">
    <div class="container">
        <div class="row">
            <div class="registration-form">
                <h2 class="text-main1 text-uppercase">Cá nhân đăng ký</h2>
                <form class="form-horizontal" role="form">
                    <p class="text-right col-md-12"><span>(*)</span> là thông tin bắt buộc nhập</p>
                    <div class="form-group">
                        <label for="name" class="control-label col-md-3 email-ico">Email <span>(*)</span></label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" placeholder="Enter name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 matkhau-ico">Mật khẩu <span>(*)</span></label>
                        <div class="col-md-9">
                            <input type="password" class="form-control" placeholder="Enter address">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 xacnhanmatkhau-ico">Xác nhận mật khẩu <span>(*)</span></label>
                        <div class="col-md-9">
                            <input type="password" class="form-control" placeholder="Enter address">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 hoten-ico">Họ tên <span>(*)</span></label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" placeholder="Enter address">
                        </div>
                        <label for="address" class="control-label col-md-2 sodienthoai-ico">Điện thoại <span>(*)</span></label>
                        <div class="col-md-3">
                            <input type="text" class="form-control" placeholder="Enter address">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 diachi-ico">Địa chỉ <span>(*)</span></label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" placeholder="Enter address">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 thanhpho-ico">Thành phố <span>(*)</span></label>
                        <div class="col-md-9">
                            <select class="selectpicker form-control">
                                <option>-- Thành phố --</option>
                                <option>1</option>
                                <option>2</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 quanhuyen-ico">Quận huyện <span>(*)</span></label>
                        <div class="col-md-9">
                            <select class="selectpicker form-control">
                                <option>-- Quận huyện --</option>
                                <option>1</option>
                                <option>2</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 phuongxa-ico">Phường xã <span>(*)</span></label>
                        <div class="col-md-9">
                            <select class="selectpicker form-control">
                                <option>-- Phường xã --</option>
                                <option>1</option>
                                <option>2</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 taikhoannganhang-ico">
                            Tài khoản ngân hàng <br />
                            (Dùng để chuyển lại tiền thu hộ cho bạn)
                        </label>
                        <div class="col-md-9">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Số tài khoản">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Chủ tài khoản (không nhập dấu)">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Chọn ngân hàng">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Chi nhánh">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <label class="checkbox-inline">
                                        <input type="checkbox" name="radio" checked="checked"> Lưu thông tin thẻ để sử ụng cho lần sau
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="address" class="control-label col-md-3 mabaove-ico">Mã bảo vệ <span>(*)</span></label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" placeholder="Enter address">
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="submit" class="basicbtn btn-main1">Đăng ký</button>
                        <button type="submit" class="basicbtn btn-main2">Nhập lại</button>
                    </div>
                    <div class="clearfix"></div>
                    <p class="text-center">
                        <span class="text-main1">Lưu ý:</span> Việc bạn chọn đăng ký tức là bạn đã đồng ý với những <a href="/" class="text-main1">
                            Chính sách &amp; điều khoản
                        </a> của FUTA Express
                    </p>
                </form>
                <div class="col-md-12 text-center">
                    <div class="btnchuyentrang">
                        <a href="/">
                            Doanh nghiệp đăng ký
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@Html.Partial("~/Views/Home/_HotlineBannerPartial.cshtml")
